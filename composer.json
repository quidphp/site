{
    "name": "quidphp/site",
    "type": "library",
    "description": "Extended platform to build a website using the QuidPHP framework and LemurCMS",
    "keywords": ["quidphp","quid","php-framework","php-cms","lemur-cms"],
    "homepage": "https://quidphp.com",
    "license": "MIT",
    "authors": [{
        "name": "Pierre-Philippe Emond",
        "email": "emondpph@gmail.com",
        "homepage": "https://emond.io",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.4.0",
        "quidphp/base": "5.30.*",
        "quidphp/main": "5.30.*",
        "quidphp/orm": "5.30.*",
        "quidphp/routing": "5.30.*",
        "quidphp/core": "5.30.*",
        "quidphp/front": "5.30.*",
        "quidphp/lemur": "5.30.*",
        "verot/class.upload.php": "^2.0",
        "phpmailer/phpmailer": "^6.6",
        "tedivm/jshrink": "^1.4",
        "scssphp/scssphp": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Quid\\Site\\": "src/",
            "Quid\\Test\\Site\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}