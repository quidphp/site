{
    "name": "quidphp/site",
    "type": "library",
    "description": "Extended platform to build a website using the QuidPHP framework and LemurCMS",
    "keywords": ["quidphp","quid","php-framework","php-cms","lemur-cms"],
    "homepage": "https://quidphp.com",
    "license": "MIT",
    "authors": [{
        "name": "Pierre-Philippe Emond",
        "email": "emondpph@gmail.com",
        "homepage": "https://emond.io",
        "role": "Developer"
    }],
    "require": {
        "php": ">=8.1.0",
        "quidphp/base": "dev-master",
        "quidphp/main": "dev-master",
        "quidphp/orm": "dev-master",
        "quidphp/routing": "dev-master",
        "quidphp/core": "dev-master",
        "quidphp/javascript": "dev-master",
        "quidphp/javascript": "dev-master",
        "quidphp/browser": "dev-master",
        "quidphp/front": "dev-master",
        "quidphp/include": "dev-master",
        "quidphp/lemur": "dev-master",
        "verot/class.upload.php": "^2.0",
        "phpmailer/phpmailer": "^6.6",
        "tedivm/jshrink": "^1.4",
        "scssphp/scssphp": "^1.10",
        "tinymce/tinymce": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Quid\\Site\\": "src/",
            "Quid\\Test\\Site\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}